{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "NODE_ENV=development nest start --watch",
    "start": "NODE_ENV=production node dist/src/main",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "typeorm migration:create -n",
    "migration:revert": "npm run build && typeorm migration:revert",
    "migration:revert:raw": "typeorm migration:revert",
    "migration:run": "npm run build && typeorm migration:run",
    "migration:run:raw": "typeorm migration:run",
    "migration:generate": "typeorm migration:generate -n",
    "migration:run:production:raw": "NODE_ENV=production typeorm migration:run --config ormconfig.production.js",
    "migration:revert:production:raw": "NODE_ENV=production typeorm migration:revert --config ormconfig.production.js"
  },
  "dependencies": {
